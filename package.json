{
	"name": "viwoods-notes-importer",
	"version": "1.0.0",
	"description": "Import Viwoods .note files directly into Obsidian with support for handwritten notes, audio recordings, and stroke data conversion to SVG.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "node esbuild.config.mjs production",
		"build:check": "tsc -noEmit && node esbuild.config.mjs production",
		"lint": "npx eslint src --ext .ts",
		"format": "npx eslint src --ext .ts --fix",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "vitest run",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest",
		"clean": "rm -f main.js manifest.json"
	},
	"keywords": ["obsidian", "obsidian-plugin", "viwoods", "notes", "import"],
	"author": "tuannvm",
	"license": "0BSD",
	"devDependencies": {
		"@types/node": "^20.0.0",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vitest": "^1.6.0",
		"@vitest/ui": "^1.6.0",
		"vite-tsconfig-paths": "^4.2.1"
	}
}
