{
	"name": "viwoods-obsidian",
	"version": "0.1.0",
	"description": "An Obsidian plugin for bidirectional synchronization with Viwoods note-taking app",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"lint": "eslint .",
		"format": "eslint . --fix",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "vitest run",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest",
		"clean": "rm -f main.js manifest.json"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"viwoods",
		"notes",
		"import"
	],
	"author": "tuannvm",
	"license": "0BSD",
	"devDependencies": {
		"@eslint/js": "^9.30.1",
		"@microsoft/eslint-plugin-sdl": "^1.1.0",
		"@types/node": "^20.0.0",
		"@typescript-eslint/eslint-plugin": "^8.35.1",
		"@typescript-eslint/parser": "^8.35.1",
		"@typescript-eslint/utils": "^8.35.1",
		"@vitest/ui": "^1.6.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.25.5",
		"eslint": "^9.18.0",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"globals": "^14.0.0",
		"jiti": "^2.6.1",
		"obsidian": "latest",
		"tslib": "^2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.35.1",
		"vite-tsconfig-paths": "^4.2.1",
		"vitest": "^1.6.0"
	},
	"overrides": {
		"eslint": "^9.18.0"
	}
}
